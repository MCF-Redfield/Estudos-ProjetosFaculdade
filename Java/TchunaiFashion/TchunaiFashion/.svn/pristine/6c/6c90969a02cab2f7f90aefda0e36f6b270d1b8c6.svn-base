body{
  background: url('../img/bg-slide-topo.png') repeat-x;
  margin: 0;
  color: #333;
  font-family: "Lucida Sans Unicode","Lucida Grande", sans-serif;
}


.container {
  margin: 0 auto;/*0p/ deixar conteudo de header colado acima e auto para centralizá-lo automaticamente*/
  width: 940px;
  }

header {
  position: relative;
  }

.menu-setores {
 
  margin-top: 20px;
  margin-left: 400px;/*posicionando à direita 'Sua Conta'...*/
  position: relative;
  bottom: 0;
  right: 0;
  }

.busca,
.menu-departamentos{
  float: left;/*Posicionamento da Busca e do Menu flutuando a esquerda*/
  background-color: rgba(172,13,32,1);/*#dcdcdc;*/
  font-weight: bold;/*letras em negrito*/
  text-transform: uppercase;/*todas letrs maiúsculas*/
  margin-right: 10px;/*espaçamento à direita do Menu*/
  width: 230px;/* |---Largura do Menu e Busca---|  */
  }

.menu-departamentos{/*é uma seção, tipo um subMenu*/
  clear: left;/*posiciona o elemento flutuante menu-departamentos a esquerda*/
  text-transform: uppercase;/*Todas as letras maiúsculas*/
  margin-top: 10px;/*espaço da margem na parte superior*/
  padding-bottom: 10px;/*distancia da parte inferior*/
  }

.item-menu{
  text-decoration: none;
  }
.busca{
  border-top-left-radius: 4px;/*borda de Busca supeiror esquerda*/
  border-top-right-radius: 4px;/*borda de Busca supeiror direita*/
  }

.busca h2,
.busca form,
.menu-departamentos h2{
  font-size: 16px;
  margin: 10px;
  color: #fff;
  }

.menu-departamentos li{
  background-color: #B22533;
  margin-bottom: 1px;
  padding: 5px 10px;
  border-radius: 2px;
  font-size: 16px;
  }

.menu-departamentos a{
  color: #fff;/*cor da fonte dos elementos da lista*/
  text-decoration: none;/*decoração ou efeito na escrita, no caso, nenhuma*/
  }

.busca input{
  vertical-align: middle;/*posicionar o texto da busca merticalmete no meio*/
  border-radius: 10px;/*borda no campo de busca com raio de 10px*/
  }

.busca input[type=search]{
  width: 170px;/*largura do campo de busca*/
  }

.destaque{
  margin-top: 100px;/*espaçamento da margem na parte superior*/
  position: relative;/*permite o posicionameto do elemento dentro da página*/
  }

.painel{
  margin: 10px 0;/*espaço de qualquer coisa acima ou abaixo do painel*/
  padding: 10px;/*espaçao da margem*/
  width: 921px;/*largura do painel*/
  border-radius: 4px;/*borda dos paineis*/
  box-shadow: 1px 1px 4px #999;/*sombreamento dos paineis*/
  }

.novidades{
  float: left;/*painel novidades flutuando à esquerda*/
  }

/*.mais_vendidos{
  float: right;/*painel mais_vendidos flutuando à direita*/
  /*}*/

.painel li{
  display: inline-block;/*dispõeos elementos do painel alinhados em bloco*/
  vertical-align: auto;/*alinha os elementos do painel na parte superior*/
  width: 140px;/*largura de cada elemento*/
  margin: 20px;/*margem dos elementos*/
  padding: 20px ;/*espaço entre os elementos do painel*/
  -webkit-transition: 0.7s ease-out;
  transition: 0.7s ease-out;
  }

.painel h2 {
  font-size: 24px;/*tamanho da fonte do titulo do painel*/
  font-weight: bold;/*formatação da fonte em negrito*/
  text-transform: uppercase;/*todas as letras em maiúsculo*/
  margin-bottom: 10px;/*margem inferior*/
  text-shadow: 1px 1px 2px rgba(255,255,255,0.8);/*sombreamento do titulo dos paineis*/
  }

.painel a {
  color: #333;/*cor da fonte no elemento do painel*/
  font-size: 14px;/*tamanho da fonte*/
  text-align: center;/*posicionamento centralizado do texto */
  text-decoration: none;/*decoração ou efeito na escrita, no caso, nenhuma*/
  }

.novidades {
  background-color: #f5dcdc;/*cor de fundo do painel*/
  background: linear-gradient(#f5dcdc, #f4bebe);/*gradiente ao painel*/
  }

