.pause,
.play {
  display: block;/*formata os botões em bloco*/
  position: absolute;/*posisciona os botões de acordo com o elemento pai*/
  right: 15px;/*posição a direita*/
  top: 15px;/*posição a parte superior*/
  }

.pause{/*desenha o botão do Pause*/
  border-left: 10px solid #900;
  border-right: 10px solid #900;
  height: 30px;
  width: 5px;
  }

.play{/*desenha o botão do Play*/
  border-left: 25px solid #900;
  border-bottom: 15px solid transparent;
  border-top: 15px solid transparent;
  }

.menu-setores a:hover{/*efeito ao passar o mouse*/
  color: #007dc6;
  }

.menu-setores a:active{/*efeito ao clicar sobre*/
  color: #867dc6;
  }

.menu-departamentos ul li ul{
  display: none;/*sublista permanece oculta*/
  }

.menu-departamentos ul li:hover ul{
  display: block;/*exibe sublista ao passar o mouse*/
  }

.menu-departamentos ul li ul li{
    background-color: #b24142;/*cor da sublista*/
  }

.menu-departamentos li li a:before{
  content: '\272A';/*símbolo para o elemento da sublista*/
  padding-right: 3px;/*espaçamento a direita do espaço destinado a opção da sublista*/
  }

.painel h2:before{
  content: '\2756';
  padding-right: 5px;
  opacity: 0.4;
  }

figure{
  background-color: #f2eded;/*cor de fundo das figuras*/
  border: 0px groove #ccc;/*borda das imagens*/
  text-align: center;/*alinhamento das imagens*/
  padding: 1px;/*espaçamento da margem*/
  margin: 1px;/*distancia da margem*/
  width: 130px;/*largura das imagens e legendas*/
  }

figcraption{
  font-style: italic;/*legenda da imagem em itálico*/
  margin-top: 10px;/*margem da legenda*/
  }

figure img{
  width:128px;/*largura das imagens*/
  }

.painel li:hover {/*efeito ao passar o mouse sobre os elementos dos paineis*/
  background-color: rgba(255,255,255,0.8);
  box-shadow: 0 0 5px #333;
  -webkit-transition: 0.7s;
  transition: 0.7s;
  -webkit-transform: scale(1.2);/*zoom ao passar o mouse*/
  transform: scale(1.2);
  -webkit-transform: scale(1.2) rotate(-5deg);
  transform: scale(1.2) rotate(-5deg);/*elemento rotaciona a -5 graus*/
}

.painel li:nth-child(2n):hover {
-webkit-transform: scale(1.2) rotate(5deg);
transform: scale(1.2) rotate(5deg);
}

.painel li:hover {
-webkit-transition: 0.7s ease-in;
transition: 0.7s ease-in;
}
