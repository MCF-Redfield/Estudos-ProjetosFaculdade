<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tchunaisoft.com.br.betweenactivities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BetweenMainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Tela1Activity"
            android:label="@string/title_activity_tela1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Tela2Activity"
            android:label="@string/title_activity_tela2"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
